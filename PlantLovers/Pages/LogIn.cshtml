@page
@model PlantLovers.Pages.LogInModel
@{
}


    <h4>@TempData["Message"]</h4>

<form enctype="multipart/form-data" method="post">

    <div class="form-group">
        <label asp-for="userCredentials.UserName"></label>
        <input asp-for="userCredentials.UserName" class="form-control">
        <span class="text-danger" asp-validation-for="userCredentials.UserName"></span>
    </div>
    <div class="form-group">
        <label asp-for="userCredentials.Password"></label>
        <input asp-for="userCredentials.Password" class="form-control">
        <span class="text-danger" asp-validation-for="userCredentials.Password"></span>
    </div>
    <button type="submit" class="btn btn-primary">Log-In</button>

    
</form>
